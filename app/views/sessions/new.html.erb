<div class="">
  <%= form_for(:session, url: login_path) do |f| %>
    <h4><%= f.label :email %></h4>
    <h4><%= f.text_field :email %></h4>
    <h4><%= f.label :password %></h4>
    <h4><%= f.password_field :password %></h4>
    <%= f.submit "Login", class: "btn btn-default btn-font-size" %>
  <% end %>
</div>
<div>
  <h4>New User?</h4>
  <button><a href="<%= new_user_path %>" class="">Sign Up</a></button>
</div>
